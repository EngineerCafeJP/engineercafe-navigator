{
  "name": "engineer-cafe-navigator",
  "version": "1.0.0",
  "description": "Engineer Cafe Navigator - NextJS Frontend + Python LangGraph Backend",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev:frontend": "cd frontend && pnpm dev",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev": "concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\"",
    "build:frontend": "cd frontend && pnpm build",
    "install:frontend": "cd frontend && pnpm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install": "pnpm install:frontend && pnpm install:backend",
    "lint:frontend": "cd frontend && pnpm lint",
    "test:frontend": "cd frontend && pnpm test",
    "test:backend": "cd backend && pytest"
  },
  "devDependencies": {
    "concurrently": "8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0",
    "python": ">=3.11.0"
  },
  "keywords": [
    "nextjs",
    "langgraph",
    "python",
    "ai-agent",
    "monorepo"
  ],
  "author": "EngineerCafeJP",
  "license": "ISC"
}
